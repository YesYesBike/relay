#!/usr/bin/perl

use warnings;
use strict;
use utf8;
binmode(STDIN, ":encoding(UTF-8)");
binmode(STDOUT, ":encoding(UTF-8)");

my ($미모리의메모리얼을감상하는데정신이팔려메모리누수가일어났다이때책임은누구에게있을까요, $슈로딩거의고향이어디인지는컴파일타임에결정될수없다) = @ARGV;
my @대망의64_97시즌FA컵결승전토키유나이티드와도킹원더러스의경기많은관심부탁드립니다;
my %유즈넷은유저네트워크의줄임말이지유즈가넷있다는말로받아들여서는안된다;
my @히후미라는철학자를아시나요모른다고요모르면알아와야죠;

sub 단치히로여행을떠난치히로뭐라고요그단스크라고요죄송합니다 {
	my ($모모이는모이를모아서모임을만들었다가모의고사를망쳤다, $조심해히나타가나타났다아이에에에낙타낙타왜) = @_;
	my %조심해히나타가나타났다아이에에에낙타낙타왜 = %{$조심해히나타가나타났다아이에에에낙타낙타왜};
	my @아이리씨아이리시위스키는민트초코랑마시면안돼요;

	$조심해히나타가나타났다아이에에에낙타낙타왜{$모모이는모이를모아서모임을만들었다가모의고사를망쳤다}++;

	for (@{$유즈넷은유저네트워크의줄임말이지유즈가넷있다는말로받아들여서는안된다{pop @{[split '', $모모이는모이를모아서모임을만들었다가모의고사를망쳤다]}}}) {
		next if $조심해히나타가나타났다아이에에에낙타낙타왜{$_}++;
		my @네루는미카를낳은적이없지만어째서인지어머니소리를듣고있다 = @{ 단치히로여행을떠난치히로뭐라고요그단스크라고요죄송합니다($_, \%조심해히나타가나타났다아이에에에낙타낙타왜) };
		@아이리씨아이리시위스키는민트초코랑마시면안돼요 = @네루는미카를낳은적이없지만어째서인지어머니소리를듣고있다 if @네루는미카를낳은적이없지만어째서인지어머니소리를듣고있다 > @아이리씨아이리시위스키는민트초코랑마시면안돼요;
	}

	unshift @아이리씨아이리시위스키는민트초코랑마시면안돼요, $모모이는모이를모아서모임을만들었다가모의고사를망쳤다;
	return \@아이리씨아이리시위스키는민트초코랑마시면안돼요;
}

while (<STDIN>) {
	chomp;
	push @대망의64_97시즌FA컵결승전토키유나이티드와도킹원더러스의경기많은관심부탁드립니다, $_;
	push @{$유즈넷은유저네트워크의줄임말이지유즈가넷있다는말로받아들여서는안된다{shift @{[split '', $_]}}}, $_;
}

if ($미모리의메모리얼을감상하는데정신이팔려메모리누수가일어났다이때책임은누구에게있을까요) {
	my $len = $슈로딩거의고향이어디인지는컴파일타임에결정될수없다 ? ($슈로딩거의고향이어디인지는컴파일타임에결정될수없다-$미모리의메모리얼을감상하는데정신이팔려메모리누수가일어났다이때책임은누구에게있을까요+1) : 1;
	@대망의64_97시즌FA컵결승전토키유나이티드와도킹원더러스의경기많은관심부탁드립니다 = (splice @대망의64_97시즌FA컵결승전토키유나이티드와도킹원더러스의경기많은관심부탁드립니다, ($미모리의메모리얼을감상하는데정신이팔려메모리누수가일어났다이때책임은누구에게있을까요-1), $len);
}

for (@대망의64_97시즌FA컵결승전토키유나이티드와도킹원더러스의경기많은관심부탁드립니다) {
	my @아까내가뭐라그랬어아카네가뭐라그랬어라고말했어;
	my %조심해히나타가나타났다아이에에에낙타낙타왜;

	@아까내가뭐라그랬어아카네가뭐라그랬어라고말했어 = @{ 단치히로여행을떠난치히로뭐라고요그단스크라고요죄송합니다($_, \%조심해히나타가나타났다아이에에에낙타낙타왜) };

	my $아카링하이유루유리하지마루요유리유라라라라유루유리유리유라라라라유루유리유리유라라라라유루유리다이지케은 = 0;
	for (@아까내가뭐라그랬어아카네가뭐라그랬어라고말했어) {
		print "$_->";
		print "\n" if $아카링하이유루유리하지마루요유리유라라라라유루유리유리유라라라라유루유리유리유라라라라유루유리다이지케은++ % 10 == 9;
	}
	print "total:$아카링하이유루유리하지마루요유리유라라라라유루유리유리유라라라라유루유리유리유라라라라유루유리다이지케은\n";

	@히후미라는철학자를아시나요모른다고요모르면알아와야죠 = @아까내가뭐라그랬어아카네가뭐라그랬어라고말했어 if @아까내가뭐라그랬어아카네가뭐라그랬어라고말했어 > @히후미라는철학자를아시나요모른다고요모르면알아와야죠;
}

my $아카링하이유루유리하지마루요유리유라라라라유루유리유리유라라라라유루유리유리유라라라라유루유리다이지케은 = 0;
print "######결과######\n";
for (@히후미라는철학자를아시나요모른다고요모르면알아와야죠) {
	print "$_->";
	print "\n" if $아카링하이유루유리하지마루요유리유라라라라유루유리유리유라라라라유루유리유리유라라라라유루유리다이지케은++ % 10 == 9;
}
print "total:$아카링하이유루유리하지마루요유리유라라라라유루유리유리유라라라라유루유리유리유라라라라유루유리다이지케은\n";
