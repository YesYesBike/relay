#!/usr/bin/perl

use utf8;
binmode(STDIN, ":encoding(UTF-8)");
binmode(STDOUT, ":encoding(UTF-8)");

my %trans = (
	"유즈넷은유저네트워크의줄임말이지유즈가넷있다는말로받아들여서는안된다" => "head",
	"대망의64_97시즌FA컵결승전토키유나이티드와도킹원더러스의경기많은관심부탁드립니다" => "std_list",
	"네루는미카를낳은적이없지만어째서인지어머니소리를듣고있다" => "arr",
	"히후미라는철학자를아시나요모른다고요모르면알아와야죠" => "res",
	"단치히로여행을떠난치히로뭐라고요그단스크라고요죄송합니다" => "find_longest",
	"모모이는모이를모아서모임을만들었다가모의고사를망쳤다" => "val",
	"조심해히나타가나타났다아이에에에낙타낙타왜" => "seen",
	"아이리씨아이리시위스키는민트초코랑마시면안돼요" => "ret",
	"아까내가뭐라그랬어아카네가뭐라그랬어라고말했어" => "longest",
	"아카링하이유루유리하지마루요유리유라라라라유루유리유리유라라라라유루유리유리유라라라라유루유리다이지케은" => "num",
	"미모리의메모리얼을감상하는데정신이팔려메모리누수가일어났다이때책임은누구에게있을까요" => arg1,
	"슈로딩거의고향이어디인지는컴파일타임에결정될수없다" => arg2,
);

while (<STDIN>) {
	my $str = $_;

	for (keys %trans) {
		$str =~ s/$_/$trans{$_}/g if $str =~ /$_/;
	}

	print $str;
}
